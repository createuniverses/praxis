print2(getFunction(LMBDown))
function LMBDown(x,y)
  --showTrace()
  --print(getMp3Time())
  WidgetLib.callAllInRange("lmbdown")
end

print2(getFunction(test))
if test == nil then print("its nil") end

do
  local test
  function test(...)
    local t = {...}
    print2(#t)
  end
  test(1,2,3,4)
  test(1,2,3)
  test(1)
end
4
3
1


print2(inspect(test))

setCamPos_old = setCamPos

setCamPos_orig = setCamPos



function setCamPos(x,y,z)
  newBuffer()
  clearTrace()
  print("scp " .. x .. "," .. y .. "," .. z)
  print(debug.traceback())
  setBufferText(getTraceText())
  clearTrace()
  setCamPos = setCamPos_old
end

setCamPos(10,10,10)


clearTrace()

